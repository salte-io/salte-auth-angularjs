{
  "name": "salte-auth-angular",
  "version": "0.0.0-semantically-released.0",
  "main": "dist/salte-auth-angular.js",
  "description": "Provides OpenID Connect support to AngularJS applications through the addition of a simple configuration.",
  "homepage": "https://github.com/salte-io/salte-auth-angular",
  "bugs": "https://github.com/salte-io/salte-auth-angular/issues",
  "contributors": [
    "Dave Woodward <dave@salte.io>",
    "Ceci <nick@salte.io"
  ],
  "keywords": [
    "salte-auth-angular"
  ],
  "engines": {
    "node": ">= 5.4.1 < 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/salte-io/salte-auth-angular.git"
  },
  "scripts": {
    "tdd": "karma start",
    "build": "webpack --progress --colors -d",
    "eslint": "eslint src/*.js tests/specs/*.spec.js",
    "test": "karma start --single-run",
    "check-coverage": "istanbul check-coverage --statements 93 --branches 89 --functions 92 --lines 92",
