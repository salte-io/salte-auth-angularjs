    "angular-mocks": "^1.5.8",
    "angular-resource": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-ui-router": "^0.4.1",
    "babel-core": "^6.18.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^4.1.2",
    "coveralls": "2.13.2",
    "cz-conventional-changelog": "^2.0.0",
    "deindent": "^0.1.0",
    "eslint": "^4.1.0",
    "eslint-config-google": "^0.9.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-mocha": "^4.11.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
